{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2017",
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "lib"
  },
  "compileOnSave": true,
  "include": [
    "src"
  ],
  "exclude":[
    "node_modules",
    "lib"
  ]
}